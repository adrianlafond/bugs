  "bugs": {
    "url": "https://github.com/adrianlafond/bugs/issues"
  },
  "homepage": "https://github.com/adrianlafond/bugs#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@adrianlafond/geom": "^1.0.4"
